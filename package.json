{
  "name": "templatenodets",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "dotenv -e .env.test jest -i",
    "d:dev": "docker-compose -f docker/docker-compose.yaml up",
    "d:down": "docker-compose -f docker/docker-compose.yaml down",
    "dev": "export NODE_OPTIONS=\"--max-old-space-size=4096\" && NODE_PATH=./src ts-node-dev --poll --respawn --pretty --transpile-only src/index.ts",
    "start": "export NODE_OPTIONS=\"--max-old-space-size=8192\" && NODE_PATH=dist/ node ./dist/index.js",
    "drb": "docker build -t \"aspback\" . -f ./Dockerfile && docker run -d -p 80:80 --name aspback -v \"aspback:/var/log\" --env-file ./env ",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "prepare": "if [[ $NODE_ENVIRONMENT != \"production\" ]]; then husky install; fi",
    "studio": "prisma studio",
    "prisma:generate": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.19.3",
    "@godaddy/terminus": "^4.11.2",
    "@prisma/client": "4.3.1",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/method-override": "^0.0.32",
    "@types/node": "^17.0.35",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-jest": "^29.0.2",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.16.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.6.2",
    "prisma": "4.3.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix src/",
      "git add ."
    ],
    "*.js": "eslint --cache --fix"
  }
}
